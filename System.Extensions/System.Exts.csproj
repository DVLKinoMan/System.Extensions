<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Authors>David Dvali</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/DVLKinoMan/System.Exts</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DVLKinoMan/System.Exts</RepositoryUrl>
    <PackageId>SystemNET.Exts</PackageId>
    <Product>SystemNET.Exts</Product>
    <Version>3.3.1</Version>
    <PackageReleaseNotes>New Features for Type Extensions</PackageReleaseNotes>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageOutputPath>C:\Custom Offline Packages</PackageOutputPath>
    <Nullable>enable</Nullable>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <AssemblyVersion>3.3.1</AssemblyVersion>
    <FileVersion>3.3.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Update="license.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="Readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
